# API Keys (only what's needed for quality)
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# GCP Configuration
GCP_PROJECT_ID=your_project_id
GCP_REGION=us-central1
GCS_BUCKET=your_bucket_name

# Firebase/Firestore (uses Application Default Credentials)
FIRESTORE_COLLECTION=personalization_jobs

# Processing Settings
TEMP_DIR=/tmp/personalize
OUTPUT_DIR=./output

# Quality Settings (high quality defaults)
VIDEO_CRF=18
AUDIO_BITRATE=192k
